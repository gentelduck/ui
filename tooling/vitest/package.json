{
  "name": "@gentleduck/vitest-config",
  "version": "0.0.1",
  "description": "this is a package for `vitest` test configurations",
  "private": true,
  "type": "module",
  "types": "index.ts",
  "main": "index.ts",
  "keywords": ["vitest", "vitest-config"],
  "author": "gentleduck",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist .cache .turbo node_modules .next .velite",
    "clean:git": "git clean -xdf .cache .turbo node_modules dist",
    "format": "biome format --write ./",
    "lint": "biome lint --write ./"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.8.0",
    "vitest": "catalog:",
    "@vitest/ui": "catalog:",
    "@vitest/coverage-v8": "catalog:"
  },
  "devDependencies": {
    "@gentleduck/typescript-config": "workspace:*"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
