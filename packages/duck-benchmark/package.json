{
  "name": "@gentelduck/duck-ui",
  "version": "0.0.2",
  "description": "Add components to your apps.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "wildduck (https://github.com/wildduck2)",
  "repository": {
